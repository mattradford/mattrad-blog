<ol reversed class="postlist">
{% for post in postslist | reverse %}
  <li class="postlist-item{% if post.url == url %} postlist-item-active{% endif %}">
    {{ post.data.date | yearDate }}
    {% if post.data.external_url %}
    <a href="{{ post.data.external_url | url }}" class="postlist-link">
    {% else %}
    <a href="{{ post.url | url }}" class="postlist-link">{% endif %}{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a>
  </li>
{% endfor %}
</ol>
